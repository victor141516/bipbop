{
  "name": "bipbop",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc && babel dist -d dist",
    "run-compiled": "node",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit"
